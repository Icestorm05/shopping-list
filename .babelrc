{
    "presets": [
        [
            "@babel/env",
            { "useBuiltIns": "usage" }
        ],
        "@babel/react",
        "@babel/typescript"
    ],
    "plugins": [
        [
            "@babel/proposal-class-properties",
            { "loose": true }
        ],
        "@babel/syntax-dynamic-import",
        [
            "@babel/plugin-proposal-decorators",
            { "legacy": true }
        ]
    ]
}